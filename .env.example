# Supabase configuration (required)
SUPABASE_ACCESS_TOKEN=
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_DB_URL=

# Optional defaults
NEXT_PUBLIC_NETWORK=testnet
DEMO_SEED_PASSWORD=

# Hedera credentials (optional)
# Account ID -> e.g. 0.0.123456 (from portal.hedera.com)
HEDERA_OPERATOR_ACCOUNT_ID=
# DER Encoded Private Key (More Details -> DER)
HEDERA_OPERATOR_DER_PRIVATE_KEY=
# Run topic-create.ts
HEDERA_TOPIC_ID=

# Optional messaging providers
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM_NUMBER=
AT_API_KEY=
AT_USERNAME=

# Optional AI summariser (used by supOS cold-chain flow)
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_API_URL=https://api.openai.com/v1/chat/completions

# supOS bridge configuration
SUPOS_BRIDGE_ENABLED=false
SUPOS_MQTT_URL=mqtt://localhost:1883
SUPOS_MQTT_USERNAME=
SUPOS_MQTT_PASSWORD=
SUPOS_BRIDGE_QOS=1
SUPOS_CLIENT_ID=
SUPOS_BRIDGE_CONCURRENCY=8
SUPOS_BRIDGE_MAX_ATTEMPTS=8
SUPOS_BRIDGE_BASE_DELAY_MS=1000
